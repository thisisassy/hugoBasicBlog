header .head__inner{display:flex;justify-content:space-between;align-items:center;padding:70px 0}header .head__inner .siteTitle-link{font-size:18px;font-weight:400;text-decoration:none;color:#333}header .head__inner .navbar .list-item{display:inline-block;margin:0 8px 0 0}footer .foot__inner{display:flex;justify-content:space-between;padding:70px 0;align-items:center}footer .in-content .logo{font-size:14px}footer .in-content .caption{font-size:12px}footer .in-content .linkContainer li{display:inline-block;margin:0 8px 0 0;font-size:14px}footer .out-content .copyright{font-size:14px}#l-content{width:auto;margin:0 16px}@media screen and (min-width: 960px){#l-content{width:1000px;margin:auto}}#l-content__inner{display:flex;flex-flow:column}@media screen and (min-width: 960px){#l-content__inner{flex-flow:row wrap}}#l-content__inner .l-grid{margin:0 8px 0 0;padding:8px 16px}#l-content__inner .l-grid .heading{margin:0 0 16px;font-size:16px;color:#333}#l-content__inner .column-06{width:calc(100% - 50%)}#l-content__inner .column-04{width:calc(100% - 70%)}*{margin:0;padding:0}html,body{font-family:-apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen, Ubuntu, Cantarell, "Open Sans", "Helvetica Neue", YuGothic, "Yu Gothic", sans-serif}li{list-style:none}li a{text-decoration:none;color:#333}
